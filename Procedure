CREATE OR REPLACE PROCEDURE E_CUSTOMER_DETAILS(
    CUSTOMER_ID IN INT,
    CUSTOMER_NAME IN CHAR,
    MOBILE_NUMBER IN INT,
    EMAIL_ADDRESS IN VARCHAR,
    ADDRESS IN VARCHAR
)
IS
BEGIN
    INSERT INTO CUSTOMER_DETAILS VALUES (CUSTOMER_ID, CUSTOMER_NAME, MOBILE_NUMBER, EMAIL_ADDRESS, ADDRESS);
   END;
/
